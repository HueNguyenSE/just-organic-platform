\treturn (\n\t\t<>\n\t\t\t<Link className='btn btn-light my-3' to='/'>\n\t\t\t\tGo Back\n\t\t\t</Link>\n            <Row>\n                <Col md={6}>\n                    <Carousel thumbWidth={100} >\n                        {images}\n                    </Carousel>\n                </Col>\n\n                <Col md={3}>\n                    <ListGroup  variant='flush'>\n                        <ListGroup.Item >\n                            <h3>{product.name}</h3>\n                        </ListGroup.Item>\n                        <ListGroup.Item >\n                            <Rating value={product.rating} text={`${product.numReviews} reviews`} />\n                        </ListGroup.Item>\n                        <ListGroup.Item >\n                            Price from: ${minPrice} per {product.uom}\n                        </ListGroup.Item>\n                        <ListGroup.Item >\n                            Min qty. per order: {minQty} {product.uom}\n                        </ListGroup.Item>\n                        <ListGroup.Item >\n                            Shipping area: {product.shippingArea}\n                        </ListGroup.Item>\n                        <ListGroup.Item >\n                            Origin: {product.origin}\n                        </ListGroup.Item>\n                        <ListGroup.Item >\n                            Description: {product.description}\n                        </ListGroup.Item>\n                        <ListGroup.Item >\n                            Provider: {product.seller}\n                        </ListGroup.Item>\n                    </ListGroup>\n                </Col>\n\n                <Col md={3}>\n                    <Card>\n                        <ListGroup variant='flush'>\n                            <ListGroup.Item>\n                                <Row>\n                                    <Col>\n                                        Price from:\n                                    </Col>\n                                    <Col>\n                                        <strong>${minPrice}</strong>\n                                    </Col>\n                                </Row>\n                            </ListGroup.Item>\n                            <ListGroup.Item>\n                                <Row>\n                                    <Col>\n                                        Status:\n                                    </Col>\n                                    <Col>\n                                        {product.countInStock > 0 ? 'In Stock' : 'Out of Stock'}\n                                    </Col>\n                                </Row>\n                            </ListGroup.Item>\n                            <ListGroup.Item>\n                               <Button className='btn-block' type='button' disabled={product.countInStock === 0}>Add To Cart</Button>\n                            </ListGroup.Item>\n                        </ListGroup>\n                    </Card>\n                </Col>\n\n            </Row>\n\t\t</>\n\t);\n}\n\nexport default ProductPage;\n","/home/hue/Documents/general-assembly/projects/just-organic-platform/client/src/index.js",[],[],[],"/home/hue/Documents/general-assembly/projects/just-organic-platform/client/src/App.js",[],[],"/home/hue/Documents/general-assembly/projects/just-organic-platform/client/src/components/Header.js",[],[],"/home/hue/Documents/general-assembly/projects/just-organic-platform/client/src/components/Footer.js",[],[],"/home/hue/Documents/general-assembly/projects/just-organic-platform/client/src/components/Product.js",[],[],"/home/hue/Documents/general-assembly/projects/just-organic-platform/client/src/components/Rating.js",[],[],"/home/hue/Documents/general-assembly/projects/just-organic-platform/client/src/pages/ProductPage.jsx",["196","197"],[],"import React, { useState, useEffect } from 'react';\nimport { Link, useParams, useNavigate } from 'react-router-dom';\nimport { useDispatch, useSelector } from 'react-redux';\nimport {\n\tRow,\n\tCol,\n\tListGroup,\n\tCard,\n\tButton,\n\tForm,\n\tFormGroup,\n} from 'react-bootstrap';\nimport 'react-responsive-carousel/lib/styles/carousel.min.css'; // requires a loader\nimport { Carousel } from 'react-responsive-carousel';\nimport { listProductDetails } from '../actions/productActions';\nimport Loader from '../components/Loader';\nimport Message from '../components/Message';\nimport Rating from '../components/Rating';\n\nfunction ProductPage() {\n\tconst [qty, setQty] = useState();\n\tconst [unitPrice, setUnitPrice] = useState();\n\n\tlet { id } = useParams();\n\n\tlet navigate = useNavigate();\n\n\tconst dispatch = useDispatch();\n\n\tconst productDetails = useSelector((state) => state.productDetails);\n\n\tconst { loading, error, product } = productDetails;\n\n\tconst images = product.images?.map((image, index) => (\n\t\t<div key={index}>\n\t\t\t<img\n\t\t\t\tsrc={image.url}\n\t\t\t\talt={product.name}\n\t\t\t\tstyle={{ aspectRatio: '1/1', objectFit: 'cover' }}\n\t\t\t></img>\n\t\t</div>\n\t));\n\n\t// duplicate code here. TODO: refactor\n\tconst startPrice = product.priceRanges?.reduce(\n\t\t(startPrice, range) =>\n\t\t\tstartPrice < range.unitPrice ? startPrice : range.unitPrice,\n\t\tInfinity\n\t);\n\n\tconst maxPrice = product.priceRanges?.reduce(\n\t\t(maxPrice, range) => range.unitPrice > maxPrice ? range.unitPrice : maxPrice, 0\n\t);\n\n\n\tconst minQty = product.priceRanges?.reduce(\n\t\t(minQty, range) => (minQty < range.minQty ? minQty : range.minQty),\n\t\tInfinity\n\t);\n\n\n\tuseEffect(() => {\n\t\tdispatch(listProductDetails(id));\n\t}, [dispatch, id, qty, unitPrice]);\n\n\tconst handleQtyChange = (e) => {\n\t\tsetQty(e.target.value);\n\t\tconsole.log(qty);\n\t\t// Users enter Qty, unit price will be calculated accordingly.\n\t\tconst priceMaps = new Map();\n\t\tconst qtyArray = [];\n\t\tproduct.priceRanges?.forEach((range) => {\n\t\t\tpriceMaps.set(range.minQty, range.unitPrice);\n\t\t\tqtyArray.push(range.minQty);\n\t\t});\n\n\t\tlet index = 0;\n\t\tfor (let i = 0; i < qtyArray.length; i++) {\n\t\t\tif (qty >= qtyArray[i] && i > index) {\n\t\t\t\tindex = i;\n\t\t\t}\n\t\t}\n\t\tsetUnitPrice(priceMaps.get(qtyArray[index]));\n\t};\n\n\tconst addToCartHandler = () => {\n\t\tlet path = `/cart/${id}?qty=${qty}`\n\t\tnavigate(path);\n\t}\n\n\n\treturn (\n\t\t<>\n\t\t\t<Link className='btn btn-light my-3' to='/'>\n\t\t\t\tGo Back\n\t\t\t</Link>\n\t\t\t{loading ? (\n\t\t\t\t<Loader />\n\t\t\t) : error ? (\n\t\t\t\t<Message variant='danger'>{error}</Message>\n\t\t\t) : (\n\t\t\t\t<Row>\n\t\t\t\t\t<Col md={6}>\n\t\t\t\t\t\t<Carousel thumbWidth={100}>{images}</Carousel>\n\t\t\t\t\t</Col>\n\n\t\t\t\t\t<Col md={3}>\n\t\t\t\t\t\t<ListGroup variant='flush'>\n\t\t\t\t\t\t\t<ListGroup.Item>\n\t\t\t\t\t\t\t\t<h3>{product.name}</h3>\n\t\t\t\t\t\t\t</ListGroup.Item>\n\t\t\t\t\t\t\t<ListGroup.Item>\n\t\t\t\t\t\t\t\t<Rating\n\t\t\t\t\t\t\t\t\tvalue={product.rating}\n\t\t\t\t\t\t\t\t\ttext={`${product.numReviews} reviews`}\n\t\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t</ListGroup.Item>\n\t\t\t\t\t\t\t<ListGroup.Item>\n\t\t\t\t\t\t\t\tPrice from: ${startPrice} per {product.uom}\n\t\t\t\t\t\t\t</ListGroup.Item>\n\t\t\t\t\t\t\t<ListGroup.Item>\n\t\t\t\t\t\t\t\tMin qty. per order: {minQty} {product.uom}\n\t\t\t\t\t\t\t</ListGroup.Item>\n\t\t\t\t\t\t\t<ListGroup.Item>\n\t\t\t\t\t\t\t\tShipping area: {product.shippingArea}\n\t\t\t\t\t\t\t</ListGroup.Item>\n\t\t\t\t\t\t\t<ListGroup.Item>Origin: {product.origin}</ListGroup.Item>\n\t\t\t\t\t\t\t<ListGroup.Item>\n\t\t\t\t\t\t\t\tDescription: {product.description}\n\t\t\t\t\t\t\t</ListGroup.Item>\n\t\t\t\t\t\t\t<ListGroup.Item>Provider: {product.seller}</ListGroup.Item>\n\t\t\t\t\t\t</ListGroup>\n\t\t\t\t\t</Col>\n\n\t\t\t\t\t<Col md={3}>\n\t\t\t\t\t\t<Card>\n\t\t\t\t\t\t\t<ListGroup variant='flush'>\n\t\t\t\t\t\t\t\t<ListGroup.Item>\n\t\t\t\t\t\t\t\t\t<Row>\n\t\t\t\t\t\t\t\t\t\t<Col>Status:</Col>\n\t\t\t\t\t\t\t\t\t\t<Col>\n\