t._id === id);\n\n    const priceRanges = new Map(product.priceRanges);\n\n    let minPrice = Infinity;\n    let minQty = Infinity;\n\n    priceRanges.forEach((price, qty) => {\n        if (parseFloat(price) < minPrice) {\n            minPrice = price;\n        }\n        if (parseFloat(qty) < minQty) {\n            minQty = qty;\n        }\n    } );\n\n\n\n\n    const images = product.images.map((image, index) => (\n            <div key={index}>\n                <img src={image} alt={product.name} />\n            </div>\n        ))\n\n\treturn (\n\t\t<>\n\t\t\t<Link className='btn btn-light my-3' to='/'>\n\t\t\t\tGo Back\n\t\t\t</Link>\n            <Row>\n                <Col md={6}>\n                    <Carousel thumbWidth={100} >\n                        {images}\n                    </Carousel>\n                </Col>\n\n                <Col md={3}>\n                    <ListGroup  variant='flush'>\n                        <ListGroup.Item >\n                            <h3>{product.name}</h3>\n                        </ListGroup.Item>\n                        <ListGroup.Item >\n                            <Rating value={product.rating} text={`${product.numReviews} reviews`} />\n                        </ListGroup.Item>\n                        <ListGroup.Item >\n                            Price from: ${minPrice} per {product.uom}\n                        </ListGroup.Item>\n                        <ListGroup.Item >\n                            Min qty. per order: {minQty} {product.uom}\n                        </ListGroup.Item>\n                        <ListGroup.Item >\n                            Shipping area: {product.shippingArea}\n                        </ListGroup.Item>\n                        <ListGroup.Item >\n                            Origin: {product.origin}\n                        </ListGroup.Item>\n                        <ListGroup.Item >\n                            Description: {product.description}\n                        </ListGroup.Item>\n                        <ListGroup.Item >\n                            Provider: {product.seller}\n                        </ListGroup.Item>\n                    </ListGroup>\n                </Col>\n\n                <Col md={3}>\n                    <Card>\n                        <ListGroup variant='flush'>\n                            <ListGroup.Item>\n                                <Row>\n                                    <Col>\n                                        Price from:\n                                    </Col>\n                                    <Col>\n                                        <strong>${minPrice}</strong>\n                                    </Col>\n                                </Row>\n                            </ListGroup.Item>\n                            <ListGroup.Item>\n                                <Row>\n                                    <Col>\n                                        Status:\n                                    </Col>\n                                    <Col>\n                                        {product.countInStock > 0 ? 'In Stock' : 'Out of Stock'}\n                                    </Col>\n                                </Row>\n                            </ListGroup.Item>\n                            <ListGroup.Item>\n                               <Button className='btn-block' type='button' disabled={product.countInStock === 0}>Add To Cart</Button>\n                            </ListGroup.Item>\n                        </ListGroup>\n                    </Card>\n                </Col>\n\n            </Row>\n\t\t</>\n\t);\n}\n\nexport default ProductPage;\n","/home/hue/Documents/general-assembly/projects/just-organic-platform/client/src/index.js",[],[],[],"/home/hue/Documents/general-assembly/projects/just-organic-platform/client/src/App.js",[],[],"/home/hue/Documents/general-assembly/projects/just-organic-platform/client/src/components/Header.js",["143"],[],"/home/hue/Documents/gen